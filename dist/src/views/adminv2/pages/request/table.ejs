<table id="example2" class="table table-bordered table-hover table-striped" style="width: 100%">
    <thead>
      <tr>
        <th style="width: 7%">No</th>
        <th>Profile Picture</th>
        <th>Username</th>
        <th>Status</th>
        <th style="width: 20%">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < usersInRequestStream.length; i++){ %>
        <tr>
          <td><%= i + 1 %></td>
          <td>
            <% if (usersInRequestStream[i].profilePicture && usersInRequestStream[i].profilePicture.startsWith('http')) { %>
              <!-- Menampilkan gambar dari URL HTTPS -->
              <img 
                src="<%= usersInRequestStream[i].profilePicture %>" 
                alt="Profile Picture" 
                class="img-thumbnail" 
                style="width: 100px; height: 100px; border-radius: 100%;" 
              />
            <% } else if (usersInRequestStream[i].profilePicture) { %>
              <!-- Menampilkan gambar dari folder /uploads -->
              <img 
                src="/uploads/<%= usersInRequestStream[i].profilePicture %>" 
                alt="Profile Picture" 
                class="img-thumbnail" 
                style="width: 100px; height: 100px; border-radius: 100%;" 
              />
            <% } %>
          </td>
          <td><%= usersInRequestStream[i].username %></td>
          <td>
            <%= usersInRequestStream[i].status %>
        </td>
        
        
          <td>
              <a href="/admin/request/detail/<%= usersInRequestStream[i].id %>" class="btn btn-primary">
                <i class="fas fa-eye"></i> View
              </a>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>
  
